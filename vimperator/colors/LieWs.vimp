highlight CompTitle>* color: #000000; background: #b2a089; border-bottom: 1px solid #5f5f8f; padding: 1px 0.5ex; text-shadow: 1px 1px 2px #0f0f2f; font-size: 14px;
highlight CompMsg font-weight: none; margin-left: 12px;
highlight CompItem font-size: 12px;
highlight CompItem[selected] font-weight: bold; color: #cfcfcf; background: #050c0f; font-size: 13px;
highlight CompItem>* height: 18px; min-height: 18px; padding: 0 0.5ex;
highlight CompIcon width: 16px; min-width: 16px; display: inline-block; margin-right: .5ex;
highlight CompIcon>img max-width: 16px; max-height: 16px; vertical-align: middle;
highlight CompResult width: 500px; max-width: 500px; overflow: hidden;
highlight CompDesc width: 500px; max-width: 500px;
